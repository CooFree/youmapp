<!--pages/login/login.wxml-->
<wxs src="../../utils/tools.wxs" module="tools" />
<view class="container ">
    <view class="g-row">
        <view class="login-form">
            <view class="inputwrap">
                <view class="inputBox">
                    <input type="text" class="j-inputtext lophone" placeholder="用户名/手机号/邮箱" maxlength="100" value="" />
                    <view class="utip">
                        <i class="icon u-clear ion-ios-close"></i>
                    </view>
                </view>
            </view>
            <view class="inputwrap">
                <view class="inputBox">
                    <input type="password" class="j-inputtext lophone" placeholder="密码" maxlength="100" value="" />
                    <view class="utip">
                        <i class="icon u-clear ion-ios-close"></i>
                    </view>
                </view>
            </view>
            <view class="inputwrap" style="display:none">
                <view class="inputBox scode">
                    <input type="text" class="j-inputtext lophone" placeholder="验证码" maxlength="100" value="" />
                    <view class="utip">
                        <i class="icon u-clear ion-ios-close"></i>
                    </view>
                    <view class="getbox">
                        <image class="authcode" src="https://app.you.camel.com.cn/upload/auth_code/13827848-e6e0-449c-a2be-bb695390c524.jpg" alt=""></image>
                    </view>
                </view>
            </view>
            <view class="btn-login">
                <text class="ben-login-txt">登录</text>
            </view>
            <view class="sublime">
                <text class="link">快速注册</text>
                <text class="link">忘记密码</text>
            </view>
            <view class="ologin">
                <a href="https://app.you.camel.com.cn/handlers/qqLogin.ashx" class="oitem">
                    <image class="icon" mode="widthFix" src="data:image/png;base64,"
                        alt=""></image>
                    <text class="txt">QQ</text>
                </a>
                <a href="https://app.you.camel.com.cn/handlers/sinaLogin.ashx" class="oitem">
                    <image class="icon" mode="widthFix" src="data:image/png;base64,"
                        alt=""></image>
                    <text class="txt">微博</text>
                </a>
            </view>
        </view>
    </view>
</view>